<workflowGraph name="">
    <param name="gusConfigFile"/>
    <param name="parentDataDir"/> 
    <param name="organismAbbrev"/>
    <param name="relativeWebServicesDir"/>
    <param name="experimentName"/>
    <param name="fileType"/>
    <param name="copyOnly"/>
    <param name="version"/>

    <step name="copyToWebServices" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::CopyManualDeliveryToWebService">
        <paramValue name="gusConfigFile">$$gusConfigFile$$</paramValue>
        <paramValue name="inputsDir">$$parentDataDir$$</paramValue>
        <paramValue name="organismAbbrev">$$organismAbbrev$$</paramValue>
        <paramValue name="relativeDir">$$relativeWebServicesDir$$</paramValue>
        <paramValue name="experimentName">$$experimentName$$</paramValue>
        <paramValue name="fileType">$$fileType$$</paramValue>
    </step>
    
    <step name="insertGFF3" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::InsertGFF3" excludeIf="$$copyOnly$$" >
        <paramValue name="gusConfigFile">$$gusConfigFile$$</paramValue>
        <paramValue name="inputsDir">$$parentDataDir$$</paramValue>
        <paramValue name="organismAbbrev">$$organismAbbrev$$</paramValue>
        <paramValue name="experimentName">$$experimentName$$</paramValue>
        <paramValue name="fileType">$$fileType$$</paramValue>
        <paramValue name="version">$$version$$</paramValue>
        <depends name="copyToWebServices"/>
    </step>

</workflowGraph>
