 <workflowGraph name="ontologySynonyms">
  <param name="parentDataDir"/>
  <param name="datasetLoaderXmlFileName"/>

  <datasetTemplate class="ontologySynonyms">
    <prop name="name"/>    

    <subgraph name="Ontology_Synonyms_${name}_RSRC" xmlFile="loadDataset.xml">
      <paramValue name="datasetName">Ontology_Synonyms_${name}_RSRC</paramValue>
      <paramValue name="datasetLoaderXmlFileName">$$datasetLoaderXmlFileName$$</paramValue>
      <paramValue name="parentDataDir">$$parentDataDir$$</paramValue>
    </subgraph>
  </datasetTemplate>

</workflowGraph>
